@model  Vouchers.BalanceAccount
<h3>Create Account</h3> <a asp-controller="Account" asp-action="Index">Back</a>

<form action="/Account/Create" method="post">
    <div class="form-horizontal">
        <div class="text-danger"></div>
        <div class="form-group">
            <label class="col-md-2 control-label" for="AccountID">AccountID</label>
            <div class="col-md-10">
                <input class="form-control" type="datetime" readonly
                       id="AccountID" name="AccountID" value="" />               
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-2 control-label" for="AccountName">AccountName</label>
            <div class="col-md-10">
                <input class="form-control" type="text"
                       data-val="true" data-val-required="The AccountName field is required."
                       id="AccountName" name="AccountName" value="" />
                <span class="text-danger field-validation-valid"
                      data-valmsg-for="AccountName" data-valmsg-replace="true"></span>
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-2 control-label" for="AccountExpense">AccountExpense</label>
            <div class="col-md-10">
                <input class="form-control" type="checkbox" id="AccountExpense" name="AccountExpense" value="" />
            </div>
        </div>
        <div >
            <input type="submit" value="Create Account"/>
            <br/>
        </div>
    </div>
</form>